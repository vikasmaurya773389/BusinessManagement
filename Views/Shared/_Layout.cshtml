
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="icon" href="https://freepngimg.com/download/business/70298-management-business-icons-consultant-company-social-marketing.png" />

    <title>@ViewData["Title"] - Business Management</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/BusinessManagement.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" integrity="sha512-iecdLmaskl7CVkqkXNQ/ZH/XLlvWZOJyj7Yy7tcenmpD1ypASozpmT/E0iPtmFIB46ZmdtAc9eNBvH0H/ZpiBw==" crossorigin="anonymous" referrerpolicy="no-referrer" />



</head>
<body>
    
    <div class="container-flude" style="min-height:675px;">
               


        <!-- Topbar Start -->
        <div class="container-fluid bg-primary text-white d-none d-lg-flex sticky-top">
            <div class="container py-3">
                <div class="d-flex align-items-center">
                    <a asp-area="" asp-controller="Account" asp-action="Index">
                        <h2 class="text-white fw-bold m-0">BUSINESS MANAGEMENT</h2>
                    </a>
                    <div class="ms-auto d-flex align-items-center">
                        
                        <div class="ms-3 d-flex">
                            <a href="https://www.facebook.com/AnthemInfotech" target="_blank" class="btn btn-sm-square btn-light text-primary rounded-circle ms-2" >
                                <i class="fab fa-facebook-f"></i> </a>

                                   
                            <a class="btn btn-sm-square btn-light text-primary rounded-circle ms-2" href="https://twitter.com/antheminfotech" target="_blank">
                                <i class="fab fa-twitter"></i></a>
                            <a class="btn btn-sm-square btn-light text-primary rounded-circle ms-2" href="https://www.linkedin.com/company/anthem-infotech-pvt-ltd-" target="_blank">
                                <i class="fab fa-linkedin-in"></i></a>
                        </div>
                        @*<a asp-controller="Account" asp-action="index"  class="btn ms-4 btn-light text-primary rounded-pill py-2 px-3"><b>@Context.Session.GetString("Name")</b></a>*@
                        <a asp-controller="Account" asp-action="index" class="nav-link dropdown-toggle btn ms-4 btn-light text-primary rounded-pill py-2 px-3"  id="navbarDropdownMenuLink" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                            <b>@Context.Session.GetString("Name")</b>
                        </a>
                         @if (Context.Session.GetString("UserEmail") == null)
                        {
                            <ul class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">


                                <li><a class="dropdown-item" asp-controller="Account" asp-action="ChangePassword">Change Password</a></li>


                            </ul>
                        }

                    </div>
                </div>
            </div>
        </div>
        <!-- Topbar End -->


        <!-- Navbar Start -->
        <div class="container-fluid bg-white mt-3 ">
            <div class="container">
                <nav class="navbar navbar-expand-lg bg-white navbar-light p-lg-0">
                    <a href="index.html" class="navbar-brand d-lg-none">
                        <h1 class="fw-bold m-0">BUSINESS MANAGEMENT</h1>
                    </a>
                    <button type="button" class="navbar-toggler me-0" data-bs-toggle="collapse"
                        data-bs-target="#navbarCollapse">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarCollapse">
                        <div class="navbar-nav">

                            <a asp-area="" asp-controller="Account" asp-action="Index" class="nav-item nav-link active">Home</a>
                            <a asp-area="" asp-controller="bids" asp-action="Index" class="nav-item nav-link">Bids</a>
                            <a asp-area="" asp-controller="Client" asp-action="Index" class="nav-item nav-link">Clients</a>
                            <a asp-area="" asp-controller="Project" asp-action="Index" class="nav-item nav-link">Projects</a>
                            @if (Context.Session.GetString("UserTypeId") == "2")
                            {
                                <a asp-area="" asp-controller="User" asp-action="Index" class="nav-item nav-link">Users</a>
                                
                            }
                           
                             
                           
                        </div>
                        <div class="ms-auto d-none d-lg-block">
                            <a onclick="window.location.href = '/Account/LogOut'" class="btn btn-primary rounded-pill py-2 px-3">Log Out</a>
                        </div>
                    </div>
                </nav>
            </div>
        </div>
        <hr />
        <!-- Navbar End -->

            
        <div class="container">
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>

    </div>

    <div class="container-fluid bg-dark  mt-5 py-5 wow fadeIn" data-wow-delay="0.1s" style="animation-delay: 0.1s; animation-name: fadeIn;">
        <div class="container">
            <div class="row">
                <div class="col-lg-4 text-light col-md-6">
                    <fieldset>
                        <legend>
                            <center> <i class="fa fa-map-marker-alt"> </i> </center>
