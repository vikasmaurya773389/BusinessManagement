                        <p style="">
                            #11, Floor 11, Sushma Infinium,
                            Chandigarh - Ambala Highway,
                            Near Best Price
                        </p>
                        <p>Zirakpur, Punjab – 140603 India.</p>
                            </a>
                        </center>
                    </fieldset>
                   
                </div>
                <div class="col-lg-4 text-light col-md-6">
                    <fieldset>
                         <legend>
                            <center> <i class="fa fa fa-user"> </i></center>
                        </legend>
                        <center>
                            <p>Phone - <a style="text-decoration:none;" class="text-light" href="tel:+91 9815-34-0123">+91 9815-34-0123</a></p>
                        <p>Email - <a style="text-decoration:none;" class="text-light" href="mailto:info@antheminfotech.com">info@antheminfotech.com</a></p>
                        </center>
                    </fieldset>
                </div>
                <div class="col-lg-4 text-light col-md-6">
                   
                  <fieldset>
                      <legend>

                            <center><i class="footerinfo-icn fa fa-share-alt icons-white"></i></center>
                      </legend>
                      <center>
                        <a href="https://www.facebook.com/AnthemInfotech" target="_blank" style="text-decoration: none; color:dodgerblue" ;><i class="fa-brands fa-facebook fa-2xl"></i></a>&nbsp;&nbsp;&nbsp;
                        <a href="https://twitter.com/antheminfotech" target="_blank" style="text-decoration: none; color: rgb(4, 174, 241);"><i class="fa-brands fa-twitter fa-2xl"></i></a>&nbsp;&nbsp;&nbsp;
                        <a href="https://plus.google.com/+Antheminfotechindia/posts" target="_blank" style="text-decoration: none; color: red" ;><i class="fa-brands fa-google fa-2xl"></i></a>&nbsp;&nbsp;&nbsp;
                        <a href="https://www.linkedin.com/company/anthem-infotech-pvt-ltd-" target="_blank" style="text-decoration: none; color: cadetblue"><i class="fa-brands fa-linkedin fa-2xl"></i></a>&nbsp;&nbsp;&nbsp;
                        <a target="_blank" href="https://wa.me/919815340123/?text=Hi Anthem Team." title="Share on whatsapp" style="text-decoration: none; color: green"><i class="fa-brands fa-whatsapp fa-2xl"></i></a>
                     </center>
                    </fieldset>
                </div>
                
            </div>
        </div>
    </div>
            <div class="copyright py-4" style="background-color:#0d6efd;">
                <div class="container">
                    <div class="row">
                        <div class="col-md-6 text-center text-light text-md-start mb-3 mb-md-0">
                    © <a class="fw-medium text-light" href="#"> 2023 Anthem Infotech Private Limited</a>, All Right Reserved.
                        </div>
                        <div class="col-md-6 text-center text-light text-md-end">
                        
                            Powered by <span class="pwdbyanthmbtns1">
                                <a href="http://antheminfotech.com/" style="color: white;" target="_blank">Anthem Infotech Pvt. Ltd.</a>
                        </span>
                        </div>
                    </div>
                </div>
            </div>


        
   
    
  
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
